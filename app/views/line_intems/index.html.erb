<p id="notice"><%= notice %></p>

<h1>Line Intems</h1>

<table>
  <thead>
    <tr>
      <th>Product</th>
      <th>Cart</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @line_intems.each do |line_intem| %>
      <tr>
        <td><%= line_intem.product %></td>
        <td><%= line_intem.cart %></td>
        <td><%= link_to 'Show', line_intem %></td>
        <td><%= link_to 'Edit', edit_line_intem_path(line_intem) %></td>
        <td><%= link_to 'Destroy', line_intem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Line Intem', new_line_intem_path %>
